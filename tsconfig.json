{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es6", "es2015.promise", "es2015.iterable", "scripthost"],
    "module": "commonjs",
    "strictNullChecks": false,
    "noImplicitAny": true,
    "sourceMap": true,
    "noEmit": true,
    "jsx": "react",
    "typeRoots": ["typings"],
    "moduleResolution": "node"
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ]
}
